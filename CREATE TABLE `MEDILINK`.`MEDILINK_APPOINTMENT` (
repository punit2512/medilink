CREATE TABLE `MEDILINK`.`MEDILINK_APPOINTMENT` (
  `APPOINTMENT_ID` INT NOT NULL,
  `PATIENT_ID` VARCHAR(45) NULL,
  `PRACTICE_ID` VARCHAR(45) NULL,
  `CONSULTANT_ID` VARCHAR(45) NULL,
  `APPOINTMENT_DATE` DATE NULL,
  `START_TIME` TIME NULL,
  `END_TIME` VARCHAR(45) NULL,
  `INS_TS` TIMESTAMP NULL,
  `UPD_TS` TIMESTAMP NULL,
  `INS_LOGIN` VARCHAR(20) NULL,
  `UPD_LOGIN` VARCHAR(20) NULL,
  PRIMARY KEY (`APPOINTMENT_ID`));